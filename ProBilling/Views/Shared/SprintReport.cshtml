@model IEnumerable<SprintReportViewModel>
<table class="table table-condensed table-striped table-responsive">
	<thead>
	<tr data-item-id="">
		<th>
			@Html.DisplayNameFor(model => model.UserName)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Designation)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.TotalLeaves)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.LeaveDates)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.BillableHours)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.BackupHours)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.CompanyMeetingHours)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.NonBillableHours)
		</th>
	</tr>
	</thead>

	<tbody>
	@foreach (SprintReportViewModel item in Model)
	{
		<tr data-item-id="">
			<td>
				@Html.DisplayFor(model => item.UserName)
			</td>
			<td>
				@Html.DisplayFor(model => item.Designation)
			</td>
			<td>
				@Html.DisplayFor(model => item.TotalLeaves)
			</td>
			<td>
				@Html.DisplayFor(model => item.LeaveDates)
			</td>
			<td>
				@Html.DisplayFor(model => item.BillableHours)
			</td>
			<td>
				@Html.DisplayFor(model => item.BackupHours)
			</td>
			<td>
				@Html.DisplayFor(model => item.CompanyMeetingHours)
			</td>
			<td>
				@Html.DisplayFor(model => item.NonBillableHours)
			</td>
		</tr>
	}
	</tbody>
</table>
